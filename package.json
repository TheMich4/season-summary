{
  "name": "season-summary-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "app:build": "bun run --cwd packages/app build",
    "app:dev": "bun run --cwd packages/app dev",
    "server:dev": "bun run --cwd packages/server dev",
    "server:start": "bun run --cwd packages/server start"
  }
}
